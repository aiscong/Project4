 <% provide(:title, "New Gallery") %>
 <%= form_for(@gallery) do |f| %>
 <%= render 'shared/error_messages', object: f.object %>
  <p>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </p>
  <p><%= f.submit "Create Gallery", class: "btn btn-large btn-primary" %></p>
<% end %>

<p><%= link_to "Back to User", user_path(current_user) %></p>